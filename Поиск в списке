-- Список с именами
names = {"Реда", "Миша", "Данил",
  "Архип", "Глеб", "Адель",
  "Алексей", "Денис", "Матвей"}
-- Спрашиваем какое имя искать
print("Введите имя для поиска в списке")
-- *l озночает читаем строчку из консоли (все до символа \n)
n = io.read("*l")
-- Счётчик
i = 1
-- Цикл для обхода нашего списка
while not (names[i] == nil) do 
  -- Если элемент списка под номером i совпадает
  -- с тем именем, которого мы искали
  if names[i] == n then
    -- То выводим что мы его нашли
    print("Я нашел в списке", n)
  end
  -- Счётчик увеличиваем на 1
  i = i + 1
  end
